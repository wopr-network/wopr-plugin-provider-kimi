{
  "dependencies": {
    "@moonshot-ai/kimi-agent-sdk": "^0.1.4",
    "@wopr-network/plugin-types": "^0.2.0",
    "winston": "^3.19.0"
  },
  "description": "Kimi provider with auto-installation (requires Python 3.12+)",
  "devDependencies": {
    "@biomejs/biome": "^2.4.3",
    "@types/node": "^25.3.2",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "wopr",
    "wopr-plugin",
    "provider",
    "kimi"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "name": "@wopr-network/wopr-plugin-provider-kimi",
  "scripts": {
    "build": "tsc",
    "check": "biome check --config-path=. src/ && tsc --noEmit",
    "format": "biome format --config-path=. --write src/",
    "lint": "biome check --config-path=. src/",
    "lint:fix": "biome check --config-path=. --fix src/",
    "postinstall": "node install-deps.mjs",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "type": "module",
  "version": "1.6.0",
  "wopr": {
    "plugin": {
      "category": "provider",
      "description": "Moonshot AI Kimi Code CLI - auto-installs Python 3.12, uv, and kimi-cli",
      "name": "provider-kimi"
    },
    "systemDependencies": {
      "note": "Will auto-install via uv if missing",
      "python": ">=3.12"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "release": {
    "extends": "@wopr-network/semantic-release-config"
  }
}
