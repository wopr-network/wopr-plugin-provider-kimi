{
  "dependencies": {
    "@moonshot-ai/kimi-agent-sdk": "^0.1.4",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "description": "Kimi provider with auto-installation (requires Python 3.12+)",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^25.2.3",
    "@wopr-network/plugin-types": "^0.1.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "wopr",
    "wopr-plugin",
    "provider",
    "kimi"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@wopr-network/wopr-plugin-provider-kimi",
  "peerDependencies": {
    "wopr": "^0.0.1"
  },
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "postinstall": "node install-deps.mjs",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "type": "module",
  "version": "1.6.0",
  "wopr": {
    "plugin": {
      "category": "provider",
      "description": "Moonshot AI Kimi Code CLI - auto-installs Python 3.12, uv, and kimi-cli",
      "name": "provider-kimi"
    },
    "systemDependencies": {
      "note": "Will auto-install via uv if missing",
      "python": ">=3.12"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
